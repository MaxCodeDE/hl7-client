{{#each model as |log|}}
<div class="row">
    <div class="col s12">
        <div class="card">
            <div class="card-content">
                <span class="card-title">{{log.file}}</span>
                <p class="log-card-header">Message:</p>
                <p>{{log.message}}</p>
                <p class="log-card-header">Ack</p>
                <p>{{log.ack}}</p>
            </div>
            <div class="card-action">
                <a class="clickable" onclick={{action (route-action 'resend' log)}}>Resend</a>
                <a class="clickable" onclick={{action (route-action 'deleteLog' log)}}>Delete</a>
            </div>
        </div>
    </div>
</div>
{{else}}
<div class="row">
    <div class="col s12">
        <div class="card">
            <div class="card-content">
                <span class="card-title">No log found</span>
                <p>There is no log messages at this moment.</p>
            </div>
        </div>
    </div>
</div>
{{/each}}